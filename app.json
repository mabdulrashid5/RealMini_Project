{
  "expo": {
    "name": "ROAD WATCH",
    "slug": "road-watch",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "roadwatch",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "backgroundColor": "#1E88E5"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "app.rork.road-safety-reporter-app",
      "config": {
        "googleMapsApiKey": "AIzaSyD6f4kc1k-aa0s1ndZWNpXiGkjj2CU_qgE"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "app.rork.road-safety-reporter-app",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyD6f4kc1k-aa0s1ndZWNpXiGkjj2CU_qgE"
        }
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://rork.com/"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "unstable_transformImportMeta": true
    }
  }
}